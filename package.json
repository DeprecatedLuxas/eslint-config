{
  "name": "@deprecatedluxas/shared-eslint-config",
  "version": "0.1.2",
  "private": true,
  "description": "My shareable ESLint config",
  "author": "Lucas Norgaard <lucasnrgaard@gmail.com>",
  "license": "MIT",
  "keywords": [
    "eslint-config",
    "eslint",
    "config"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/deprecatedluxas/eslint-config.git"
  },
  "scripts": {
    "lint": "eslint .",
    "bump": "bumpp package.json packages/*/package.json --commit --push --tag",
    "release": "pnpm -r publish --access public"
  },
  "devDependencies": {
    "@deprecatedluxas/eslint-config": "*",
    "bumpp": "^7.1.1",
    "eslint": "^8.16.0",
    "typescript": "^4.7.2"
  }
}
